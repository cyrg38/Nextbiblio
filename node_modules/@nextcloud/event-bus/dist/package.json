        "core-js": "^3.6.5",
        "semver": "^7"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-typescript": "^7.10.1",
        "@nextcloud/browserslist-config": "^1.0.0",
        "babel-jest": "^26.0.1",
        "babel-plugin-inline-json-import": "^0.3.2",
